{
  "version": "2.0.0",
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "ES5",
    "module": "commonjs",
    "moduleResolution": "node",
    "removeComments": false,
    "sourceMap": true,
    "outDir": "./dist",
    "declaration": false,
    "noImplicitAny": false,
    "isolatedModules": false,
    "forceConsistentCasingInFileNames": true,
    "noEmitHelpers": true
  },
  "exclude": [
    "node_modules",
    "!node_modules/phaser",
    "!node_modules/phaser-plugin-isometric"
  ]
}

